<div class="page-header">
    <p class="pull-right">
        <a role="button" class="btn btn-success" href="@Url.Action("Edit", new { id = ViewData["Id"] })"><i class='fa fa-pencil fa-fw'></i>Edit</a>
        <a role="button" class="btn btn-danger" href="@Url.Action("Delete", new { id = ViewData["Id"] })"><i class='fa fa-trash-o fa-fw'></i> Delete</a>
    </p>
    <h2>
        <a role="button" class="btn btn-default" href="@Url.Action("Index")"><i class='fa fa-chevron-left fa-fw'></i> @ViewData["_Types"] </a>
        @ViewData["_Type"] @ViewData["_Name"]
        <small>DETAILS</small>
    </h2>
</div>